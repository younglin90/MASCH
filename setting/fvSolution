// 분할 솔버 (segregated solver)
segregated {
	nCorrectors = (10 10(1 1));
	relaxationFactors = (0.7 (0.7 0.3));
}

// 커플 솔버 (coupled solver)
coupled {
	nCorrectors = 5;
	relaxationFactors = 0.9;
}

