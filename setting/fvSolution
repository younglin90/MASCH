limiterCellVar {
	p = (1.e-5 1.e12);
	u = (-1.e12 1.e12);
	v = (-1.e12 1.e12);
	w = (-1.e12 1.e12);
	T = (-1.e12 1.e12);
	Y = (0.0 1.0);
